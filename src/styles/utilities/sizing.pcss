/* Size utility */
.w-100 {
    width: 100% !important;
}

.w-auto {
    width: auto !important;
}

.vw-50 {
    width: 50vw !important;
}

.h-100 {
    height: 100% !important;
}

/* end of size utility */


/* Fixed size utility */
.f-w-5 {
    width: 1.25rem !important;
}

.f-w-6 {
    width: 1.5rem !important;
}

.f-h-5 {
    height: 1.25rem !important;
}

.f-h-12 {
    height: 3rem !important;
}

/* end of fixed size utility */